// #text1
	ctx.fillStyle = 'rgb(0, 0, 0)';
	ctx.lineWidth = 25.831800;
	ctx.font = "309.982px";
	ctx.fillText("METANOIA", 242.514100, 626.422790);
	ctx.restore();

    </script>
